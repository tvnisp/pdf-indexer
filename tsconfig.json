{
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"target": "es2018",
		"module": "commonjs",
		"allowJs": true,
		"jsx": "react",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"composite": true,
		"noEmit": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true,
	}
}
